  <script>import tavernaPBS.*;


PBS in = PBSConvert.toPBS(inPBS);

String metaSim = in.getVar("METASIM");

if (metaSim == null) {
	metaSim = "MetaSim cmd";
}

// "/h3/t1/users/mjl3p/MetaSim/MetaSim cmd ";
//String errorModel = "/h3/t1/users/mjl3p/MetaSim/examples/errormodel-80bp.mconf";

String fastaFileName = Extract.getFile(fastaFile);

String readsFile = outputFolder + fastaFileName.replace(".fa", "-Empirical.fna");

String readCount = in.getVar("READ_COUNT");

if (readCount == null) {
	readCount = "100000";
}

String command = metaSim + " -c -mg " + errorModel + " --empirical-pe-probability 1 "
		+ " -r" + readCount + " " + fastaFile;

Job job = new Job("mkdir -p " + outputFolder);
job.addCommand("cd " + outputFolder);
job.addCommand(command);
job.addInput(fastaFile);
job.addOutput(readsFile);

job.setProcessors(2);
job.setMemory("8GB");

in.doJob(job);

String outPBS = PBSConvert.fromPBS(in);</script>
