  <script>import tavernaPBS.*;


PBS in = PBSConvert.toPBS(inPBS);

String gatkCoverage = in.getVar("GATK_COVERAGE");

if (gatkCoverage == null) {
	gatkCoverage = "java -Xmx4g -jar GenomeAnalysisTK.jar";
}

String covFile = Extract.getDirectory(bamFile) + "coverage.txt";

String command = gatkCoverage
		+ " -I " + bamFile
		+ " -R " + refFile
		+ " -L " + targets
		+ " -o " + covFile
		+ " -omitLocusTable -omitIntervals -omitSampleSummary "
		+ " -nt 4 -mmq 10";

Job job = new Job(command);
job.addInput(refFile);
job.addInput(bamFile);
job.addInput(targets);
job.addOutput(covFile);

job.setProcessors(4);

in.doJob(job);

String outPBS = PBSConvert.fromPBS(in);</script>
