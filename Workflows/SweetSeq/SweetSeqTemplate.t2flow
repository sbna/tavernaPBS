  <script>import tavernaPBS.*;

PBS head = new PBS();

head.setUser("me");
head.setClusterHost("host.college.edu");

head.startLog(logFile);

head.loadConfig(configFile);

// MetaSim tools
head.setVar("METASIM", "/path/to/metasim cmd");
head.setVar("READ_COUNT", "100000");
head.setVar("READS_SPLIT", "/path/to/readsSplit.pl");
head.setVar("FASTQ_CONVERT", "/path/to/fastqConvert.pl");

// BWA programs
head.setVar("BWA_INDEX", "/path/to/bwa index"); 
head.setVar("BWA_ALN", "/path/to/bwa aln");
head.setVar("BWA_SAMPE", "/path/to/bwa sampe");
head.setVar("BWA_BWASW", "/path/to/bwa bwasw");

// samtools
head.setVar("SAMTOOLS_INDEX", "/path/to/samtools index");
head.setVar("SAMTOOLS_FAIDX", "/path/to/samtools faidx");

// picard
head.setVar("CREATE_SEQ_DICT", "java -Xmx4g -jar /path/to/CreateSequenceDictionary.jar");

// GATK
head.setVar("GATK_COVERAGE", "java -Xmx4g -jar /path/to/GenomeAnalysisTK.jar -T DepthOfCoverage");
head.setVar("GATK_FLAGSTAT", "java -Xmx4g -jar /path/to/GenomeAnalysisTK.jar -T FlagStat");
head.setVar("GATK_COUNTLOCI", "java -Xmx4g -jar /path/to/GenomeAnalysisTK.jar -T CountLoci");

// analysis scripts
head.setVar("COVERAGE_CONVERT", "/path/to/coverageConvert.pl");
head.setVar("STAT_EXTRACT", "/path/to/statExtract.pl");



String outPBS = PBSConvert.toPBS(head);</script>
