  <script>import tavernaPBS.*;


PBS in = Merge.pbsList(inPBSs);

String statFiles = Merge.fileList(stats);

String mergedStats = outDirectory + "stats.txt";

String command = "cat " + statFiles + " &gt; " + mergedStats;

Job job = new Job(command);
job.addInput(stats);
job.addOutput(mergedStats);

in.doJob(job);

String outPBS = PBSConvert.fromPBS(in);
</script>
